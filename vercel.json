{
  "version": 3,
  "framework": "vite",
  "buildCommand": "vite build",
  "functions": {
    "api/property-gpt.ts": {
      "runtime": "nodejs20.x",
      "memory": 512,
      "maxDuration": 30
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1.ts" },
    { "src": "/(.*)", "dest": "/" }
  ]
}